[project]
	description = The main task is optimize package installation in tinycore.
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Ingwarr (ingwarr)
[access "refs/for/refs/*"]
	read = group user/Ingwarr (ingwarr)
	push = group user/Ingwarr (ingwarr)
	label-Code-Review = -2..+2 group user/Ingwarr (ingwarr)
	label-Verified = -1..+1 group user/Ingwarr (ingwarr)
	rebase = group user/Ingwarr (ingwarr)
	removeReviewer = group user/Ingwarr (ingwarr)
	submit = group user/Ingwarr (ingwarr)
[access "refs/heads/*"]
	read = group user/Ingwarr (ingwarr)
	create = group user/Ingwarr (ingwarr)
	push = +force group user/Ingwarr (ingwarr)
	pushMerge = group user/Ingwarr (ingwarr)
[access "refs/tags/*"]
	push = +force group user/Ingwarr (ingwarr)
	pushTag = group user/Ingwarr (ingwarr)
	pushSignedTag = group user/Ingwarr (ingwarr)
